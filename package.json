{
  "name": "chat-application",
  "type": "module",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "npx nx affected -t run build",
    "lint": "npx nx affected -t run lint",
    "dev": "concurrently --kill-others-on-fail \"yarn emulators:start\" \"npx nx run frontend:dev\"",
